all: CLIPDRV.EXE CLIPDRVF.EXE CLIPCLI.COM

CLIPDRV.EXE: CLIPDRV.C-- CLIPDRV.H-- CLIP.H-- XMS.H--
   ..\TOOLS\c-- /TEXE /X CLIPDRV.C--

CLIPDRVF.EXE: CLIPDRVF.C-- CLIPDRV.H-- CLIP.H-- FILE.H--
   ..\TOOLS\c-- /TEXE /X CLIPDRVF.C--

CLIPCLI.COM: CLIPCLI.C-- CLIP.H--
   ..\TOOLS\c-- /X CLIPCLI.C--

install: all
   copy CLIPDRV.EXE ..\BIN
   copy CLIPDRVF.EXE ..\BIN
   copy CLIPCLI.COM ..\BIN

clean:
   del CLIPDRV.EXE
   del CLIPDRVF.EXE
   del CLIPCLI.COM
   del *.BAK
